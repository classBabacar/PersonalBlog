{
    "name": "bobsbackend",
    "version": "1.0.0",
    "description": "bobsbackend is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "setup": "npm install && npm update",
        "start": "node src/server.js",
        "client": "cd ./../frontend && npm start",
        "server": "cd ./../backend/src && node server.js",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "keywords": [
        "express",
        "backend",
        "bobsbackend"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-custom-error": "^1.9.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.22.0",
        "http-status": "^1.3.2",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "mongodb": "^3.5.7",
        "morgan": "^1.10.0",
        "rotating-file-stream": "^1.4.6",
        "routes": "^2.1.0"
    },
    "devDependencies": {
        "concurrently": "^5.2.0"
    }
}
